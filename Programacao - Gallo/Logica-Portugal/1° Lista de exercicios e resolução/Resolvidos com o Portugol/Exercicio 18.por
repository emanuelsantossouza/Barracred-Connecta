programa {
  // Calcular a média ponderada de duas notas com pesos diferentes.
  funcao inicio() {
    real nota1,nota2, resultado

    // Entrada
    escreva("Qual é o valor 1: ")
    leia(nota1)

    escreva("Qual é o valor 1: ")
    leia(nota2)
    // Processamento
    resultado = ((nota1 + nota2) / 2)

    // Saída 
    escreva("O a sua note foi: ", resultado)
  }
}
