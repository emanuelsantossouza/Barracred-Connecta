programa {
  // Calcular a área de um triângulo dado a base e a altura.
  funcao inicio() {
    // Entrada
    real baseTriangulo, alturaTriangulo 
    
    escreva("Qual é a base do triangulo: ")
    leia(baseTriangulo)

    escreva("Qual é a altura do triangulo: ")
    leia(alturaTriangulo)

    // Processamento E Saída
    escreva("A área do retângulo é de: ", ((baseTriangulo * alturaTriangulo)/2))
  }
}
