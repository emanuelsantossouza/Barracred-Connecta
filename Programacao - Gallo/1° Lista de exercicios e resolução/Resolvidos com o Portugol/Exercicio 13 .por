programa {
  // Calcular a área de um quadrado dado o tamanho do lado.
  funcao inicio() {
    // Entrada
    real ladosDoQuadrado

    // Processamento
    escreva("Informe o valor do quadrado: ")
    leia(ladosDoQuadrado)

    // Saída
    escreva("O valor da area é de: ", ladosDoQuadrado * ladosDoQuadrado)
  }
}
