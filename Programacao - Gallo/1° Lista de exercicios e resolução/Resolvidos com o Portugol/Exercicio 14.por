programa {
  // Calcular a área de um retângulo dado o comprimento e a largura.
  funcao inicio() {
    // Entrada
    real comprimentoRetangulo, larguraRetangulo, 
    
    escreva("Qual é a base do comprimento: ")
    leia(comprimentoRetangulo)

    escreva("Qual é a largura do retângulo: ")
    leia(larguraRetangulo)
    // Processamento E Saída
    escreva("A área do retângulo é de: " comprimentoRetangulo * larguraRetangulo)

    
  }
}
