programa {
  // Calcular o quociente inteiro da divisão de dois números inteiros.
  funcao inicio() {
    real valor1, valor2, resultado

    escreva("Informe o valor1: ")
    leia(valor1)
    escreva("Informe o valor2: ")
    leia(valor2)

    resultado = valor1 / valor2

    escreva("O valor quocinte entre dois número é: ",resultado)
  }
}
