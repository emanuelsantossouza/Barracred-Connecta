programa {
  // Calcular o produto entre dois números inteiros.
  funcao inicio() {
    inteiro num1, num2, resultado

    escreva("Escreva o valor 1 para a multiplicação: ")
    leia(num1)

    escreva("Escreva o valor 2 para a multiplicação: ")
    leia(num2)

    resultado = (num1 * num2)

    escreva("O calculo do produto é: ", resultado)
  }
}

