programa {
  // Elabore um algoritmo que leia um número inteiro e mostre o valor deste número elevado ao quadrado.
  funcao inicio() {
    real valorQuadrado
    escreva("Informe o valor que será elevado ao quadrado: ")
    leia(valorQuadrado)
    
    escreva("O valor ao quadrado é: ", valorQuadrado * valorQuadrado)
  }
}
