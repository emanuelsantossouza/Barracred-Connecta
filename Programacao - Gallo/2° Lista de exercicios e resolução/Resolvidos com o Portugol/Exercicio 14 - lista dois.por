programa {
  /*
  labore um algoritmo que leia dois valores inteiros, A e B, e apresente os valores trocados, ou seja, que 
  troque o valor da variável A pelo valor da variável B e vice-versa.
 */

  funcao inicio() {
    real a,b,c

    escreva("Qual é o valor de A: ")
    leia(a)
    escreva("Qual é o valor de B: ")
    leia(b)

    escreva("O valor de A é: ", a, "\nO valor de B é: ",b)
  }
}
