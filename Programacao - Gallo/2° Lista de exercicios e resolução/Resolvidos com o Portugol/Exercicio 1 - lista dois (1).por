programa {
  // Elabore um algoritmo que leia 3 números e apresente o resultado da soma do primeiro pelo segundo multiplicado pelo terceiro.
  funcao inicio() {
    real num1, num2, num3

    escreva("Informe o 1° valor: ")
    leia(num1)

    escreva("Informe o 1° valor: ")
    leia(num2)

    escreva("Informe o 1° valor: ")
    leia(num3)

    escreva("Informe o resultado das contas: ", (num1 + num2)*num3)
  }
}
