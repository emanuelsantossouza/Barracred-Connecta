programa {
  // Escreva um algoritmo que leia três números inteiros e positivos (A, B, C) e calcule a seguinte expressão:
  funcao inicio() {
    inteiro a, b, c, r, s,d
    

    escreva("Informe o valor de A: ")
    leia(a)

    escreva("Informe o valor de A: ")
    leia(b)

    escreva("Informe o valor de A: ")
    leia(c)
    
    r = (a + b)* (a + b)
    s = (b+c)*(b+c)

    d = (r+s)/2

    escreva("O resultado da ecuação é de: ", d)
  }
}
